package Programs;
import java.util.Scanner;
public class TwoB
{
    public static void main(String [] args)
    {   String s;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the String");
        s = sc.nextLine();
        System.out.println("Enter the int");
        int n = sc.nextInt();
        System.out.println("String:"+s+"   int:"+n);
    }
}
